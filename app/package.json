{
  "name": "hello-world",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "false": "0.0.4",
    "is-mobile": "^0.3.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-gsap-enhancer": "^0.3.0",
    "react-router": "^3.0.2",
    "react-scripts": "^3.0.1",
    "react-slick": "^0.22.3"
  },
  "scripts": {
    "build-css": "node-sass-chokidar --include-path --source-map true src/styles/index.scss -o public/css",
    "watch-css": "npm run build-css && node-sass-chokidar src/styles/index.scss -o public/css --watch --recursive",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
