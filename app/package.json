{
  "name": "colin-johnson-portfolio",
  "version": "1.1.0",
  "private": true,
  "dependencies": {
    "babel-eslint": "^7.1.1",
    "eslint": "^3.16.1",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.0",
    "false": "0.0.4",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-gsap-enhancer": "^0.3.0",
    "react-router": "^3.0.2"
  },
  "devDependencies": {
    "node-sass": "^4.5.0",
    "npm-run-all": "^4.0.2",
    "react-scripts": "0.9.3"
  },
  "scripts": {
    "build-css": "node-sass-chokidar --include-path --source-map true src/styles/index.scss -o public/css",
    "watch-css": "npm run build-css && node-sass-chokidar src/styles/index.scss -o public/css --watch --recursive",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  }
}
